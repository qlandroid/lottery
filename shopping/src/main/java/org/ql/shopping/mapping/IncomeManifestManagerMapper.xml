		</if>
	</sql>
	<sql id="select">
		SELECT
		a.account
		,b.income_id
		,b.income_create_date
		,b.zhifubao_doc
		,b.pay_money
		,b.income_in_qty
		,b.income_doc_no
		,b.status
		,b.user_id
		,b.income_before_qty
		,b.income_after_qty
		,b.income_end_date
		,c.name
		,c.zhifubao
		,c.l_bi
		,c.phone
		FROM
		manifest_income b
		JOIN user_login
		a ON a.id =
		b.user_id
		JOIN user_client
		c ON c.user_id =
		b.user_id
	</sql>
</mapper>
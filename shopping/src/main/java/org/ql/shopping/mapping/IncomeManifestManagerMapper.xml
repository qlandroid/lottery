		</if>
		<if test="inQty != null and inQty != ''">
			AND b.income_in_qty = #{inQty ,jdbcType=INTEGER}
		</if>
		<if test="incomeDocNo != null and incomeDocNo != ''">
			AND b.income_doc_no = #{incomeDocNo ,jdbcType=VARCHAR}
		</if>
		<if test="status != null and status != ''">

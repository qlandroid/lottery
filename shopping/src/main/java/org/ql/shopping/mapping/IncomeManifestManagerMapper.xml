		</if>
		<if test="userId != null and userId != ''">
			AND b.user_id = #{userId ,jdbcType=INTEGER}
		</if>
		<if test="beforeQty != null and beforeQty != ''">
			AND b.income_before_qty = #{beforeQty ,jdbcType=INTEGER}
		</if>


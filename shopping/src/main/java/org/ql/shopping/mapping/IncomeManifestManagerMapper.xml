		</if>
		<if test="zhifubaoDoc != null and zhifubaoDoc != ''">
			AND b.zhifubao_doc = #{zhifubaoDoc ,jdbcType=VARCHAR}
		</if>
		<if test="payMoney != null and payMoney != ''">
